{
  "name": "page-editor",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Stepan Kurennykh <s.kurennykh@e-mba.ru>",
  "license": "MIT",
  "scripts": {
    "analyze": "source-map-explorer dist/src.*.js dist/src.*.map",
    "dev": "rm -r dist/ && parcel ./src/index.html",
    "build": "rm -r dist/ && parcel build ./src/index.html"
  },
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "ory-editor-core": "0.2.27",
    "ory-editor-plugins-default-native": "0.2.27",
    "ory-editor-renderer": "0.2.27",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "serve": "9.2.0"
  },
  "devDependencies": {
    "autoprefixer": "8.6.5",
    "babel-eslint": "8.2.5",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-preset-react-app": "3.1.2",
    "eslint": "5.0.1",
    "eslint-config-react-app": "2.1.0",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.10.0",
    "parcel-bundler": "1.9.4",
    "source-map-explorer": "1.5.0"
  },
  "postcss": {
    "modules": false,
    "plugins": {
      "autoprefixer": {
        "browsers": [
          ">1%",
          "last 4 versions",
          "Firefox ESR",
          "not ie < 9"
        ],
        "flexbox": "no-2009"
      }
    }
  }
}
